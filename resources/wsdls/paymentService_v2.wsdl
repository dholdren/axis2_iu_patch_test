        <xsd:documentation>PaymentEntities Enity and related types for Dow Jones
			Billing Functional Domain</xsd:documentation>
      </xsd:annotation>
      <xsd:import namespace="http://omc.dowjones.com/schema/common/types/currency/v1_0"/>
      <xsd:complexType name="HistoryItem">
        <xsd:sequence>
          <xsd:element name="itemPoid" type="xsd:string"/>
          <xsd:element name="createTimestamp" type="xsd:dateTime"/>
          <xsd:element name="closeTimestmap" type="xsd:dateTime"/>
          <xsd:element name="amount" type="curr:CurrencyAmount"/>
          <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ItemTransferEvent">
        <xsd:sequence>
          <xsd:element name="eventID" type="xsd:string"/>
          <xsd:element name="endDate" type="xsd:dateTime"/>
          <xsd:element name="billID" type="xsd:string"/>
          <xsd:element name="billNumber" type="xsd:string"/>
          <xsd:element name="amount" type="curr:CurrencyAmount"/>
