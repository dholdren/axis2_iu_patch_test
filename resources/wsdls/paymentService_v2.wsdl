<wsdl:definitions targetNamespace="http://omc.dowjones.com/schema/billing/servicecontracts/paymentService/v2_0" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch0="http://omc.dowjones.com/schema/billing/servicecontracts/paymentService/v2_0" xmlns:sch1="http://omc.dowjones.com/schema/common/types/currency/v1_0" xmlns:sch10="http://omc.dowjones.com/schema/common/types/salutation/v1_0" xmlns:sch11="http://omc.dowjones.com/schema/common/types/contact/v1_0" xmlns:sch12="http://omc.dowjones.com/schema/billing/entities/account/v2_0" xmlns:sch13="http://omc.dowjones.com/schema/billing/entities/receipt/v2_0" xmlns:sch14="http://omc.dowjones.com/schema/billing/entities/chargeback/v1_0" xmlns:sch2="http://omc.dowjones.com/schema/billing/entities/paymentEntities/v1_0" xmlns:sch3="http://omc.dowjones.com/schema/billing/entities/paymentSchedule/v1_0" xmlns:sch4="http://omc.dowjones.com/schema/billing/types/creditCard/v2_0" xmlns:sch5="http://omc.dowjones.com/schema/billing/types/debitCard/v2_0" xmlns:sch6="http://omc.dowjones.com/schema/billing/types/billMe/v1_0" xmlns:sch7="http://omc.dowjones.com/schema/common/types/address/v1_0" xmlns:sch8="http://omc.dowjones.com/schema/billing/entities/paymentInstrument/v2_0" xmlns:sch9="http://omc.dowjones.com/schema/billing/entities/receivable/v2_0" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://omc.dowjones.com/schema/billing/servicecontracts/paymentService/v2_0">
  <wsdl:types>
    <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://omc.dowjones.com/schema/billing/servicecontracts/paymentService/v2_0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:acct="http://omc.dowjones.com/schema/billing/entities/account/v2_0" xmlns:ar="http://omc.dowjones.com/schema/billing/entities/receivable/v2_0" xmlns:cb="http://omc.dowjones.com/schema/billing/entities/chargeback/v1_0" xmlns:curr="http://omc.dowjones.com/schema/common/types/currency/v1_0" xmlns:pe="http://omc.dowjones.com/schema/billing/entities/paymentEntities/v1_0" xmlns:ps="http://omc.dowjones.com/schema/billing/entities/paymentSchedule/v1_0" xmlns:recpt="http://omc.dowjones.com/schema/billing/entities/receipt/v2_0">
      <xsd:annotation>
        <xsd:documentation>PaymentService -The payment scheduler operations provide the
			facility to split up payment of an item into a series of
			scheduled installments. For example, an annual subscription
			costing $120 could be paid in monthly installments over the
			course of a year. The service will calculate the dates on
			which to collect the installments and schedule the payments
			in the underlying revenue management platform. In addition
			to creating a set of scheduled payments, the service also
			provides the facility to retrieve the details of said
			schedules as well as cancel them.</xsd:documentation>
      </xsd:annotation>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/paymentEntities/v1_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/paymentSchedule/v1_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/receivable/v2_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/common/types/currency/v1_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/account/v2_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/receipt/v2_0"/>
      <xsd:import namespace="http://omc.dowjones.com/schema/billing/entities/chargeback/v1_0"/>
      <xsd:element name="CreatePaymentScheduleRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="requestID" type="xsd:string"/>
            <xsd:element name="billID" type="xsd:string"/>
            <xsd:element name="initalInstallmentDate" type="xsd:dateTime"/>
            <xsd:element name="frequencyType" type="ps:PaymentFrequency"/>
            <xsd:element name="frequencyUnits" type="xsd:positiveInteger"/>
            <xsd:element minOccurs="0" name="numberInstallments" type="xsd:positiveInteger"/>
            <xsd:element minOccurs="0" name="amountPerInstallment" type="curr:CurrencyAmount"/>
            <xsd:element minOccurs="0" name="totalAmount" type="curr:CurrencyAmount"/>
            <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CreatePaymentScheduleResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="schedule" type="ps:PaymentSchedule"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetPaymentSchedulesRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element name="onlyOpenSchedules" type="xsd:boolean"/>
            <xsd:element name="onlyPendingInstallments" type="xsd:boolean"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetPaymentSchedulesResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="scheduleList" type="ps:PaymentSchedule"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CancelPaymentScheduleRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" name="scheduleIDList" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CancelPaymentScheduleResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="scheduleList" type="ps:PaymentSchedule"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CollectPaymentRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element name="billID" type="xsd:string"/>
            <xsd:element name="paymentInstrumentID" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CollectPaymentResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="paymentEventID" type="xsd:string"/>
            <xsd:element name="paymentItemID" type="xsd:string"/>
            <xsd:element name="paymentItemNumber" type="xsd:string"/>
            <xsd:element name="paymentItemName" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" name="transferEvents" type="pe:ItemTransferEvent"/>
            <xsd:element name="paymentAmount" type="curr:CurrencyAmount"/>
            <xsd:element name="currentAccountBalance" type="acct:AccountBalanceSummary"/>
            <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReprocessRefundRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
