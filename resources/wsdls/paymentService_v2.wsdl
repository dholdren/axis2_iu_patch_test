            <xsd:element minOccurs="0" name="referenceNumber" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ListReceivableHistoryResponse">
        <xsd:complexType>
