        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
    <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://omc.dowjones.com/schema/billing/entities/paymentSchedule/v1_0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:curr="http://omc.dowjones.com/schema/common/types/currency/v1_0" xmlns:tns="http://omc.dowjones.com/schema/billing/entities/paymentSchedule/v1_0">
      <xsd:annotation>
