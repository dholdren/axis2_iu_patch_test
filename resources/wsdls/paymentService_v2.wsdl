            <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CreateCreditRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
