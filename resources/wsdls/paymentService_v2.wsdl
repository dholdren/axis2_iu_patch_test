        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GenerateReceiptRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element minOccurs="0" name="paymentEventID" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GenerateReceiptResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="receipt" type="recpt:Receipt"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ProcessFullChargebacksRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" name="payments" type="cb:ChargebackRequest"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ProcessFullChargebacksResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element name="totalChargebackAmount" type="curr:CurrencyAmount"/>
            <xsd:element maxOccurs="unbounded" name="chargebacks" type="cb:ChargebackItem"/>
            <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:schema>
