            <xsd:element name="accountBalance" type="acct:AccountBalanceSummary"/>
            <xsd:element minOccurs="0" name="currency" type="curr:CurrencyType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="IssueRefundRequest">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="requestingUserUUID" type="xsd:string"/>
            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element name="reason" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="IssueRefundResponse">
        <xsd:complexType>
          <xsd:sequence>
