            <xsd:element name="accountID" type="xsd:string"/>
            <xsd:element name="itemStatus" type="ar:ReceivableItemStatusType"/>
            <xsd:element maxOccurs="unbounded" name="itemTypes" type="ar:ReceivableItemType"/>
            <xsd:element minOccurs="0" name="startDate" type="xsd:dateTime"/>
            <xsd:element minOccurs="0" name="endDate" type="xsd:dateTime"/>
