			Domain</xsd:documentation>
      </xsd:annotation>
      <xsd:import namespace="http://omc.dowjones.com/schema/common/types/currency/v1_0"/>
      <xsd:simpleType name="PaymentFrequency">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Days"/>
          <xsd:enumeration value="Weeks"/>
          <xsd:enumeration value="Months"/>
          <xsd:enumeration value="Years"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentScheduleInstallmentStatus">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Pending"/>
          <xsd:enumeration value="Paid"/>
          <xsd:enumeration value="Cancelled"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentScheduleStatus">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Active"/>
          <xsd:enumeration value="Complete"/>
          <xsd:enumeration value="Cancelled"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="PaymentScheduleInstallment">
        <xsd:sequence>
          <xsd:element name="id" type="xsd:string"/>
          <xsd:element name="status" type="tns:PaymentScheduleInstallmentStatus"/>
          <xsd:element name="scheduledDate" type="xsd:dateTime"/>
          <xsd:element name="amount" type="curr:CurrencyAmount"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PaymentSchedule">
        <xsd:sequence>
          <xsd:element name="scheduleID" type="xsd:string"/>
          <xsd:element name="billID" type="xsd:string"/>
          <xsd:element name="paymentInstrumentID" type="xsd:string"/>
          <xsd:element name="status" type="tns:PaymentScheduleStatus"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="installmentList" type="tns:PaymentScheduleInstallment"/>
          <xsd:element name="totalAmount" type="curr:CurrencyAmount"/>
        </xsd:sequence>
